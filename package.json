{
  "name": "mekbay",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "gen-meta": "node scripts/generate-build-meta.js",
    "start": "npm run gen-meta && ng serve --host 0.0.0.0",
    "build": "npm run gen-meta && ng build --configuration=production",
    "watch": "npm run gen-meta && ng build --watch --configuration=development",
    "test": "npm run gen-meta && ng test",
    "deploy": "npm run gen-meta && npm run build && node deploy.js"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.0.5",
    "@angular/common": "^20.3.6",
    "@angular/compiler": "^20.3.6",
    "@angular/core": "^20.3.6",
    "@angular/platform-browser": "^20.3.6",
    "@angular/router": "^20.3.6",
    "@angular/service-worker": "^20.3.6",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.6",
    "@angular/build": "^20.3.6",
    "@angular/cli": "^20.3.6",
    "@angular/compiler-cli": "^20.3.6",
    "@types/jasmine": "~5.1.0",
    "@types/pako": "^2.0.4",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ssh2-sftp-client": "^12.0.1",
    "typescript": "~5.8.2"
  }
}
