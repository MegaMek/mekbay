{
  "name": "mekbay",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "gen-meta": "node scripts/generate-build-meta.js",
    "gen-assets": "node scripts/compress-assets.js",
    "prerun": "npm run gen-meta && npm run gen-assets",
    "start": "ng serve --host 0.0.0.0",
